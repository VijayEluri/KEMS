import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Consolegambijava {
 public static String leia() {

   InputStreamReader is = new InputStreamReader(System.in);
   BufferedReader stdin = new BufferedReader(is);
   String retorno = null;
   try {
     retorno = stdin.readLine();
   } catch (IOException e) {
     System.out.println("Não é possível ler do console");
     e.printStackTrace();
     System.exit(1);
   }
   return retorno;
 }
}