package Proxy;

//interface
interface IPessoa {
  public String getNome();
  public String getId();
}

//objeto real
class PessoaReal implements IPessoa{
	private String _nome, _id;
	
	public String getId() {
		return null;
	}
	public String getNome() {
		return null;
	}
	
	public PessoaReal(String id, String nome){
		_id  id;
		_nome = nome;
	}
	
} 

public class ExemploProxy {

}