package Factory2;

public class CriaFactory2 {
	public enum classesConcretas {
		concrete1,
		concrete2,
		concrete3;
	}
	
	public IFact2 CriaAloMundo(classesConcretas tipo){
		if (tipo.equals(classesConcretas.concrete1)) {
			return new clsConcrete1();
		} else if (tipo.equals(classesConcretas.concrete2)) {
			return new clsConcrete2();
		} else if (tipo.equals(classesConcretas.concrete3)) {
			return new clsConcrete3();
		}
		/*if (tipo.equals(idiomas.ingles)) {
			return new EnglishAloMundo();
		} else if (tipo.equals(idiomas.espanhol)) {
			return new SpanishAloMundo();
		} else if (tipo.equals(idiomas.alemao)) {
			return new GermanAloMundo();
		}*/
		return null;

	}
	
}

